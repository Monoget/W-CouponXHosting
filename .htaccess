RewriteRule ^Blog\-Details(.*) blog-details.php$1

RewriteRule ^Blog(.*) blog.php$1

RewriteRule ^Offer offer.php [NE,L]

RewriteRule ^Savings savings.php [NE,L]

RewriteRule ^Stores(.*) stores.php$1

RewriteRule ^Terms-and-Condition terms-and-condition.php [NE,L]

RewriteRule ^Privacy-Policy privacy-policy.php [NE,L]